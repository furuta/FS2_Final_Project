<div class="intro">
    <div class="container">
      <div class="row row-eq-height">
        <!-- Intro Content -->
        <div class="col-lg-12">
          <div class="intro_content">
            <div class="section_title_container">
              <div class="section_title"><h1>Search Results:</h1></div>
            </div>
            <div class="intro_text">
                <% if(typeof data !== 'undefined') { %>
                    <% data.forEach( s => { %>
                    <div class="box">
                        <p><%= s.address.country %></p>
                        <p><%= s.address.countrySubd %></p>
                        <p><%= s.address.oneLine %></p>
                        <p><%= s.address.postal1 %></p>
                    </div>
                      
                    <% }) %>
                  <% } %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>